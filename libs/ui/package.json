{
  "name": "ui",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*",
      "bun": "./src/utils/*",
      "import": "./src/utils/*",
      "require": "./src/utils/*"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "bun": "./src/icons/index.ts",
      "import": "./src/icons/index.ts",
      "require": "./src/icons/index.ts"
    }
  },
  "files": [
    "package.json",
    "src",
    "!src/**/*.test.ts"
  ],
  "dependencies": {
    "@floating-ui/react": "^0.27.16"
  },
  "peerDependencies": {
    "bun-plugin-tailwind": "catalog:",
    "clsx": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwindcss": "catalog:"
  }
}
